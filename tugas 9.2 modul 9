rom termcolor import cprint, colored

cprint("        _________________________________________","red")
cprint("      /                                                                   \ ", "red")
cprint("    /                                                                       \ ", "red")
cprint("  /                                                                           \ ","red")
cprint("/                                                                               \ ","red")

for i in range(3):
	cprint(' '*33, 'white', 'on_white')

for i in range(3):
	cprint(' '*3, 'white','on_white', end = ' ')
	cprint(' '*3, 'white','on_dark_grey',end = ' ')
	cprint(' '*3, 'white','on_white', end = ' ')
	cprint(' '*9, 'white','on_light_blue', end = ' ')
	cprint(' '*3, 'white','on_white', end = ' ')
	cprint(' '*3, 'white','on_dark_grey', end = ' ')
	cprint(' '*3, 'white','on_white', end = ' ')
	print()

for i in range(3):
	cprint(' '*11, 'white', 'on_white', end = ' ')
	cprint(' '*9, 'white', 'on_light_blue', end = ' ')
	cprint(' '*11, 'white', 'on_white', end = ' ')
	print()
	
	
